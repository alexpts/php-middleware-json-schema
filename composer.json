{
    "name": "alexpts/php-middleware-json-schema",
    "description": "Middleware for validate request/response by json-schema format",
    "type": "library",
    "keywords": [
        "middleware",
        "validator",
        "json-schema",
        "psr-7",
        "psr-15"
    ],
    "config": {
        "optimize-autoloader": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "alexpts",
            "email": "alexpts@yandex.ru"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0",
        "opis/json-schema": "^1.1",
        "symfony/yaml": "^6.2",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.2",
        "laminas/laminas-diactoros": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "PTS\\Md\\JsonSchemaValidator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\PTS\\Md\\JsonSchemaValidator\\": "test"
        }
    }
}
